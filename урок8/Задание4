n = int(input())
maximum = 0
minimum = n
if n >= 10:
    while n > 0:
        last_n = n % 10

        if last_n > maximum:
            maximum = last_n
        n = n // 10
        if last_n < minimum:
            minimum = last_n


print("Максимальная цифра равна " + str( maximum),"Минимальная цифра равна " + str(minimum), sep="\n")
