seconds = 0
minutes = 0
hours = 0
print(hours,minutes,seconds,sep=":")
while seconds <= 59:
    seconds += 1
    if seconds == 60:
        minutes += 1
        seconds = 0
        if minutes ==60:
            hours+=1
            minutes=0
            if hours==24 and minutes ==0 and seconds ==0:
                break
    print(hours,minutes,seconds,sep=':')
